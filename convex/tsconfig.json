{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ES2021"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "strict": true,
    "noImplicitAny": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "include": ["./**/*"],
  "exclude": ["./_generated"]
}
